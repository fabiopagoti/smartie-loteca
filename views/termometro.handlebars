<div class="container">
	<h1>Termômetro</h1>
	<br>

<p class="termometro-subtitulo">Selecione 15 números: </p>

		{{#each numeros}}
		<div class="d-flex p-2 flex-row justify-content-between align-items-center flex-wrap">
			{{#each no}}
			<div class="btn-group" data-toggle="buttons">
				<label class="btn btn-outline-primary numero" >
					<div class="numero-texto">
						<input type="checkbox" enabled>{{this}}
					</div>
				</label>
			</div>
			{{/each}}
		</div>
		{{/each}}

</div>
</div>
